{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Youtube Crawler",
            "type": "node",
            "request": "launch",
            "protocol": "inspector",
            "cwd": "${workspaceRoot}/backend/cronTasks/youtubeCrawler",
            "args": ["src/index.ts"],
            "runtimeArgs": ["-r", "/usr/lib/node_modules/ts-node/register"],
            "internalConsoleOptions": "openOnSessionStart",
            "env": {
                "DEBUG": "true",
                "TS_NODE_IGNORE": "false"
            }
        }
    ]
}
